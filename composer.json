{
	"name": "baraja-core/currency-exchange-rate",
	"description": "Smart package for currency rate manipulation by ČNB.",
	"homepage": "https://github.com/baraja-core/currency-exchange-rate",
	"authors": [
		{
			"name": "Jan Barášek",
			"homepage": "https://baraja.cz"
		}
	],
	"require": {
		"php": "^8.0",
		"nette/utils": "^3.2"
	},
	"require-dev": {
		"nette/caching": "^3.0",
		"phpstan/phpstan": "^0.12.74",
		"tracy/tracy": "^2.8",
		"phpstan/phpstan-nette": "^0.12.14",
		"roave/security-advisories": "dev-master"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"scripts": {
		"phpstan": [
			"vendor/bin/phpstan analyse src -c phpstan.neon --level 8 --no-progress"
		]
	},
	"minimum-stability": "stable"
}
